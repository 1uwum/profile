(("default" . ((user-emacs-directory . (get-env "EMACSHOME"))
	       (straight-p . t)
	       (server-name . "default")))
  ("doom" . ((user-emacs-directory . (get-env "DOOMDIR"))
	     (straight-p . t)
	     (server-name . "doom")))
  ("prelude" . ((user-emacs-directory . (get-env "PRELUDEDIR"))
		(straight-p . t)
		(server-name  . "prelude"))))
